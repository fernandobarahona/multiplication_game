; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="TablasMultiplicar"
run/main_scene="res://scenes_and_scripts/main.tscn"
config/icon="res://icon.png"

[autoload]

GlobalValues="*res://scenes_and_scripts/gl_singl__global_constants/global_contants.gd"
SceneManager="*res://scenes_and_scripts/gl_singl__scene_manager/scene_manager.gd"
Firebase="*res://scenes_and_scripts/gl_singl__firebase/firebase.gd"
UserConfig="*res://scenes_and_scripts/gl_singl__user_config/user_config.gd"

[display]

window/size/width=360
window/size/height=640
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="keep"

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/depth/hdr.mobile=true
environment/default_environment="res://default_env.tres"
